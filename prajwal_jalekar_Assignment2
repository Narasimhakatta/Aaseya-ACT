    "\n",
    "\n",
    "letter_count = 0\n",
    "digit_count = 0\n",
    "\n",
    "\n",
    "for char in user_input:\n",
    "    if char.isalpha():\n",
    "        letter_count += 1\n",
    "    elif char.isdigit():\n",
    "        digit_count += 1\n",
    "\n",
    "\n",
    "print(\"Letters:\", letter_count)\n",
    "print(\"Digits:\", digit_count)\n"
